/center {
    /string exch def
    /y exch def
    /x exch def

    string dup stringwidth pop
    2 div
    x exch sub
    y moveto
} def

/square {
    /y exch def
    /x exch def
    newpath
    x y moveto
    x y 1 add lineto
    x 1 add y 1 add lineto
    x 1 add y lineto
    closepath fill
} def
